{
    "foam.openDailyNote.directory": "./src/updates",
    "foam.files.newNotePath": "currentDir",
    "deno.enable": true,
    "gitlens.remotes": [
        {
            "domain": "mau.dev",
            "type": "GitLab"
        }
    ],
    "conventionalCommits.scopes": [
        "api",
        "global",
        "mkdocs"
    ],
    "python.analysis.autoImportCompletions": true
    "json.schemaDownload.enable": true,
    "json.schemas": [
        {
            "fileMatch": [
                ".denoflare*"
            ],
            "schema": true,
            "url": "https://raw.githubusercontent.com/skymethod/denoflare/v0.5.12/common/config.schema.json"
        }
    ]
}
